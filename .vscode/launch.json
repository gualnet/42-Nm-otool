	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch ftprintf.a",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Nm/ft_nm",
			"args": ["${workspaceFolder}/Nm/lib/ft_printf/ftprintf.a"],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "lldb",
			"preLaunchTask": "make ft_nm",
		},
		{
			"name": "Launch libftall.a",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Nm/ft_nm",
			"args": ["${workspaceFolder}/Nm/lib/libftall.a"],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "lldb",
			"preLaunchTask": "make ft_nm"
		},
		{
			"name": "Launch bash",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Nm/ft_nm",
			"args": ["${workspaceFolder}/testing_files/bash"],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb",
			"preLaunchTask": "make ft_nm"
		}
	]
}